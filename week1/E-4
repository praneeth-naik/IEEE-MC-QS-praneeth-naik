class Solution {
    public int maxProfit(int[] prices) {
        int minPrice = Integer.MAX_VALUE, maxPro = 0;
        for(int price:prices){
            if(price<minPrice){
                minPrice = price;
            }
            maxPro = Math.max(maxPro, (price-minPrice));
        }
        if(maxPro>0)
            return maxPro;
        else
            return 0;
    }
}
